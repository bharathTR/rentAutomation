@model SampleProject.Models.CustomerModel

@{
    /**/

    ViewBag.Title = "ViewCustomer";



}


@using (Html.BeginForm())
{
    <section class="content-header">
        <h1>
            @ViewBag.Title

        </h1>

    </section>


    <br />

    <section class="box">
        <section class="box-body">
            <div id="viewbtn" class="text-right">
                <input type="button" value="New" name="New" id="btnNew" class="btn btn-success" onclick="window.location.href='/Customer/CreateCustomer'">
                <input type="button" value="Back" name="back" id="btnback" class="btn btn-primary" onclick="window.location.href='/Dashboard/Index'">
            </div>
            <table id="myDataTable" class="table table-striped table-bordered" cellspacing="0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>FirstName</th>
                        <th>LastName</th>
                        <th>contact</th>
                        <th>HouseNo</th>
                        <th>BlockNo</th>
                        <th>LastLoginDate</th>
                        <th>Action</th>
                    </tr>
                </thead>

            </table>
        </section>
    </section>
}


